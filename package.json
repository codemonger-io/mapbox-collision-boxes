{
  "name": "mapbox-collision-boxes",
  "version": "0.2.0",
  "description": "Utility to handle Mapbox collision boxes",
  "packageManager": "pnpm@10.12.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.mjs",
    "dist/index.d.ts",
    "dist/index.js.map",
    "dist/index.mjs.map",
    "README_ja.md"
  ],
  "scripts": {
    "build": "rollup -c && api-extractor run --local",
    "build:doc": "npm run build && api-documenter markdown --input-folder temp --output-folder api-docs/markdown",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Mapbox"
  ],
  "author": "Kikuo Emoto <kemoto@codemonger.io>",
  "license": "MIT",
  "devDependencies": {
    "@mapbox/unitbezier": "^0.0.1",
    "@microsoft/api-documenter": "^7.26.27",
    "@microsoft/api-extractor": "^7.52.8",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/geojson": "catalog:",
    "@types/mapbox-gl": "^2.7.21",
    "gl-matrix": "^3.4.3",
    "mapbox-gl": "catalog:",
    "rollup": "^4.43.0",
    "tslib": "^2.8.1",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "mapbox-gl": ">= 2 < 4"
  }
}
